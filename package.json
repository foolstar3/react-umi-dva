{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "start-test": "cross-env UMI_ENV=test umi dev",
    "start-dev": "cross-env UMI_ENV=dev umi dev",
    "start-prod": "cross-env UMI_ENV=prod umi dev",
    "build": "umi build",
    "build-test": "cross-env UMI_ENV=test umi build",
    "build-dev": "cross-env UMI_ENV=dev umi build",
    "build-prod": "cross-env UMI_ENV=prod umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-form": "^1.36.0",
    "@ant-design/pro-layout": "^6.5.0",
    "echarts": "^5.1.2",
    "http-server": "^13.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-breadcrumbs-hoc": "^4.0.1",
    "codemirror": "^5.62.2",
    "react-codemirror2": "^7.2.1",
    "umi": "^3.5.15"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "^1.8.22",
    "@umijs/test": "^3.5.15",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
